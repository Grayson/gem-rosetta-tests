#!/usr/bin/env bash

PARENT_DIRECTORY="$(cd "$(dirname "${BASH_SOURCE[0]}")"; pwd)"
source "${PARENT_DIRECTORY}/../util.sh"

main() {
	local real_tool=$(caching_real_path "$0" "$PARENT_DIRECTORY")
	
	if [[ -n $(is_rosetta) ]]; then
		arch -arm64 "$real_tool" $@
	else
		"$real_tool" $@
	fi
}

main "$@"